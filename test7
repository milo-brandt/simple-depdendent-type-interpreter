block {
  axiom Bool : Type;
  axiom yes : Bool;
  axiom no : Bool;

  declare if : (T : Type) -> Bool -> T -> T -> T;
  if _ yes yes_branch _ = yes_branch;
  if _ no _ no_branch = no_branch;

  declare weird : (b : Bool) -> (if _ b (Bool -> Bool) Bool);
  weird yes yes = no;
  weird yes no = yes;
  weird no = yes;

  weird no
}
