# TEST BEGIN
# TEST NAME Patterns attempting to match a declaration without justification fail.
# TEST FULL_SET compile_result

block {
  axiom Bool : Type;
  axiom yes : Bool;
  axiom no : Bool;

  declare not : Bool -> Bool;
  not yes = no;
  not no = yes;

  declare f : Bool -> Bool -> Bool;
  f x (not x) = yes;

  f yes yes
}

# TEST DEFINITION

REQUIRE(compile_result.holds_success());
REQUIRE(!compile_result.get_value().is_okay());
