# TEST BEGIN
# TEST NAME A dependent pattern with redundant captures is accepted and computes.
# TEST SET expr

block {
  axiom Wrap : Type -> Type;
  axiom wrap : (T : Type) -> T -> Wrap T;

  declare unwrap : (T : Type) -> Wrap T -> T;
  unwrap S (wrap T x) = x;

  unwrap U64 (wrap U64 17)
}

# TEST SET simple

17

# TEST DEFINITION

REQUIRE(expr == simple);
